type Number interface {
    <T> Add(a, b T) T
    <T> Add3(a, b, c T) T
    <T> Sub(a, b T) T
    <T> Mul(a, b T) T
    <T> Div(a, b T) T
    <T> Mod(a, b T) T
    <T> Sqrt(a T) T
    <T> String() string
}

func <Int> Add(a, b Int) Int {
    return a + b
}

func <Int> Add3(a, b, c Int) Int {
    return a + b + c
}

func <Int> Sub(a, b Int) Int {
    return a - b
}

func <Int> Mul(a, b Int) Int {
    return a * b
}

func <Int> Div(a, b Int) Int {
    return a / b
}

func <Int> Mod(a, b Int) Int {
    return a % b
}

func <Int> Sqrt(a Int) Int {
    return 0
}

